<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Regression tests in all browser" parallel="tests" thread-count="2">

	<test name="registration tests in chrome">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class
				name="com.qa.automationpractice.parallel.reg.RegistrationTests" />
		</classes>
	</test>
	
	
	<test name="registration tests in firefox">
	<parameter name="browser" value="firefox"></parameter>
		<classes>
			<class
				name="com.qa.automationpractice.parallel.reg.RegistrationTests" />
		</classes>
	</test>

	
</suite>
